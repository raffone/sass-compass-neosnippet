snippet transition (single)
	@include single-transition(${1:property}, ${2:#:duration-in-seconds}, ${3:#:ease||linear||ease-in||ease-out||ease-in-out||cubic-bezier(x1, y1, x2, y2)}, ${4:#:delay-in-seconds})${10}

snippet transition-delay
	@include transition-delay(${1:delay-in-seconds})${10}

snippet transition-duration
	@include transition-duration(${1:duration-in-seconds})${10}

snippet transition-property
	@include transition-property(${1:property})${10}

snippet transition-time-function
	@include transition-time-function(${1:ease||linear||ease-in||ease-out||ease-in-out||cubic-bezier(x1, y1, x2, y2)})${10}

snippet transition
