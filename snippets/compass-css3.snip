	@include simple-transform(${1:scale:#:unitless}, ${2:#:rotate:#:unitless}deg, ${3:#:trans-x:unit}, ${4:#:trans-y:unit}, ${5:#:skew-x:unitless}deg, ${6:#:skew-y:unitless}deg, ${7:#:origin-x:unit})${10}

snippet transform (skew)
	@include skew(${1:x:unitless}deg, ${2:y:unitless}deg, ${3:#:3D-only:true||false})${10}

snippet transform (skewX)
	@include skewX(${1:unitless}deg, ${3:#:3D-only:true||false})${10}

snippet transform (skewY)
	@include skewY(${1:unitless}deg, ${3:#:3D-only:true||false})${10}

